{
    "manifest_version": 3,
    "name": "Timer Extension",
    "version": "1.0",
    "description": "Custom timer and notification extension",
    "permissions": [
        "notifications"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png"
        }
    },
    "icons": {
        "16": "images/icon16.png"
    }
}